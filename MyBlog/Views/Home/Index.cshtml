@model IEnumerable<MyBlog.Models.ArticleModels.Article>
<a class="btn btn-success" asp-action="Create" asp-controller="Article">Create Article</a>
@foreach (var article in Model)
{
<div>
    <h2>@article.Title</h2>
    <p>@article.Text</p>
    <img alt="Alternate Text" style="width:200px;" src="data:image/jpeg;base64,@(Convert.ToBase64String(article.Photo))" />
    <p>@article.Date</p>
    <a class="btn btn-danger" asp-action="Delete" asp-controller="Article" asp-route-id="@article.Id">Delete</a>
    <a class="btn btn-warning" asp-action="Edit" asp-controller="Article" asp-route-id="@article.Id">Edit</a>
</div>
}